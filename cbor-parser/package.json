{
  "name": "@wingriders/datum-explorer-cbor-parser",
  "version": "0.0.1",
  "description": "CBOR parser, matching with CDDL AST",
  "repository": {
    "type": "git",
    "url": "git://github.com/WingRiders/datum-explorer.git",
    "directory": "cbor-parser"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
