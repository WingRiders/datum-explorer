WingRidersRequestV2 = #6.121([ oil               : int / #6.2(bytes) / #6.3(bytes)
                             , beneficiary       : Address
                             , ownerAddress      : Address
                             , compensationDatum : any
                             , datumType         : DatumType
                             , deadline          : int
                             , assetASymbol      : bytes
                             , assetAToken       : bytes
                             , assetBSymbol      : bytes
                             , assetBToken       : bytes
                             , requestAction     : RequestAction
                             , scaleA            : int
                             , scaleB            : int
                             ])

Address = #6.121([ addressCredential        : AddressCredential
                 , addressStakingCredential : MaybeStakingCredential
                 ])

AddressCredential = PubKeyCredential / ScriptCredential

PubKeyCredential = #6.121([ pubKeyHash : bytes
                          ])

ScriptCredential = #6.122([ scriptHash : bytes
                          ])

MaybeStakingCredential = JustStakingCredential / Nothing

JustStakingCredential = #6.121([ credential : StakingCredential
                               ])

StakingCredential = StakingHash / StakingPtr

StakingHash = #6.121([ credential: AddressCredential
                     ])

StakingPtr = #6.122([ blockIndex       : int / #6.2(bytes) / #6.3(bytes)
                    , txIndex          : int / #6.2(bytes) / #6.3(bytes)
                    , certificateIndex : int / #6.2(bytes) / #6.3(bytes)
                    ])

Nothing = #6.122([])

DatumType = NoDatumType / DatumHashType / InlineDatumType

NoDatumType = #6.121([])

DatumHashType = #6.122([])

InlineDatumType = #6.123([])

RequestAction = SwapAction
                 / AddLiquidityAction
                 / WithdrawLiquidityAction
                 / ExtractTreasuryAction
                 / AddStakingRewardsAction
                 / ExtractProjectTreasuryAction
                 / ExtractReserveTreasuryAction

SwapAction = #6.121([ swapDirection   : SwapDirection
                    , minWantedTokens : int / #6.2(bytes) / #6.3(bytes)
                    ])

SwapDirection = SwapAToB / SwapBToA

SwapAToB = #6.121([])

SwapBToA = #6.122([])

AddLiquidityAction = #6.122([ minWantedShares : int / #6.2(bytes) / #6.3(bytes)
                            ])

WithdrawLiquidityAction = #6.123([ minWantedA : int / #6.2(bytes) / #6.3(bytes)
                                 , minWantedB : int / #6.2(bytes) / #6.3(bytes)
                                 ])

ExtractTreasuryAction = #6.124([])

AddStakingRewardsAction = #6.125([])

ExtractProjectTreasuryAction = #6.126([])

ExtractReserveTreasuryAction = #6.127([])
